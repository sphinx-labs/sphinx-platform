{
  "name": "@sphinx-managed/website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "generate": "yarn wagmi generate && yarn prisma-generate && yarn gql",
    "initialize": "ts-node ./scripts/init.ts",
    "check:env": "dotenv -p ALCHEMY_API_KEY",
    "build:local": "yarn prisma-generate && infisical run --env=dev --path=/Vercel -- next build",
    "build:prod": "yarn workspace @sphinx-managed/utilities run build:prod && yarn prisma-generate && next build",
    "build": "next build",
    "start": "next start",
    "build:services": "echo 'no service build necessary'",
    "dev": "next dev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "gql": "yarn ts-node ./scripts/codegen/index.ts",
    "check-gql": "yarn prisma-generate && ts-node ./scripts/codegen/checkCodegen.ts",
    "prisma-generate": "prisma generate --schema ./server/prisma/schema.prisma",
    "prisma-reset": "prisma migrate reset",
    "prebuild": "yarn prisma-generate",
    "test": "yarn generate && yarn test:ci",
    "test:ci": "bash ./__tests__/run-tests.sh",
    "clean": "rimraf out/ dist/ ./tsconfig.tsbuildinfo .next broadcast cache out"
  },
  "resolutions": {
    "viem": "^2.8.13"
  },
  "workspaces": {
    "nohoist": [
      "@sphinx-labs/plugins"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@chakra-ui/icons": "^2.0.14",
    "@chakra-ui/react": "^2.4.4",
    "@chakra-ui/styled-system": "^2.6.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@eth-optimism/common-ts": "^0.7.1",
    "@fontsource/hanken-grotesk": "^4.5.0",
    "@graphql-yoga/node": "^3.1.2",
    "@metamask/jazzicon": "^2.0.0",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@next/font": "^13.0.5",
    "@pothos/core": "^3.24.0",
    "@pothos/plugin-prisma": "^3.39.0",
    "@pothos/plugin-with-input": "^3.9.7",
    "@prisma/client": "^5.5.2",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/nextjs": "^7.101.1",
    "@sphinx-labs/contracts": "^0.23.0",
    "@sphinx-labs/core": "^0.26.0",
    "@sphinx-managed/utilities": "./services/utilities",
    "@tanstack/react-query": "^5.28.4",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@web3modal/wagmi": "^4.1.1",
    "aws-sdk": "^2.1536.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.3",
    "ethers": "^6.7.0",
    "framer-motion": "^8.0.1",
    "generate-api-key": "^1.0.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-yoga": "^3.7.0",
    "next": "^13.5.0",
    "next-auth": "^4.17.0",
    "nodemailer": "^6.8.0",
    "nuqs": "^1.17.2",
    "p-limit": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "viem": "2.x",
    "wagmi": "^2.5.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/client-preset": "^1.2.4",
    "@graphql-codegen/schema-ast": "^2.6.1",
    "@jest/globals": "^29.6.3",
    "@sphinx-labs/plugins": "^0.33.0",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.0",
    "@wagmi/cli": "^2.1.2",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^29.7.0",
    "cli-table3": "^0.6.3",
    "dotenv-cli": "^7.2.1",
    "graphql-tag": "^2.12.6",
    "hardhat": "^2.17.0",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "postcss": "^8.4.12",
    "prettier": "^2.8.8",
    "prisma": "^5.5.2",
    "solady": "^0.0.133",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "*",
    "vite-node": "^1.4.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
